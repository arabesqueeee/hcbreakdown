{
  "welcomeFile": "comtsmchcbreakdown/",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/ou(.*)$",
      "destination": "mainservice",
      "csrfProtection": false,
      "authenticationType": "none"
    },
    {
      "source": "^/breakDown(.*)$",
      "destination": "mainservice",
      "csrfProtection": false,
      "authenticationType": "none"
    },
    {
      "source": "^/ouadjust(.*)$",
      "destination": "mainservice",
      "csrfProtection": false,
      "authenticationType": "none"
    },
    {
      "source": "^/user(.*)$",
      "target": "$1",
      "destination": "user_api"
    }
  ]
}